{
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {},
    "rules": {
        "strict": [2, "global"],
        "eqeqeq": [2, "smart"],
        "no-unused-vars": [
            "error",
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": true }
        ],
        "no-console": "off"
    },
    "plugins": []
}
